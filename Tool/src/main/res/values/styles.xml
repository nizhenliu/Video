<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="LargeTxtStyle" tools:ignore="ResourceName">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#333333</item>
    </style>
    <style name="NormalTxtStyle" tools:ignore="ResourceName">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">#555555</item>
    </style>
    <style name="SmallTxtStyle" tools:ignore="ResourceName">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">#555555</item>
    </style>
    <style name="InverseTxtStyle" tools:ignore="ResourceName">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>