<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GapBottomNavigationView">
        <attr name="anchor_fab" format="reference" />
        <attr name="shadow_length" format="dimension" />
        <attr name="corner_radius" format="dimension" />
        <attr name="center_radius" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="none" value="0"></enum>
            <enum  name="circle" value="1"></enum>
            <enum name="round" value="2"></enum>
        </attr>
        <attr name="radius" format="integer"></attr>
    </declare-styleable>
</resources>